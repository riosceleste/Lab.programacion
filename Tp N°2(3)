#include <iostream>

using namespace std;

int main()
{
    float PI = 3.14159;
    float radius = 5;
    float area;
    area=radius * radius * PI;
    cout<<area << endl; 
    radius=20;
    area=radius * radius * PI;
    cout<< area <<endl;
    
    //radius= 5 y 20.Se muestra 0 veces la palabra radius
 
    return 0;
}
